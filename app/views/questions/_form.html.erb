<h1>Log In</h1>
<%=form_for @question do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :question %>
<%= f.text_field :question %>
<%= f.label :description %>
<%= f.text_field :description %>
<%= f.label :language %>
<%= f.text_field :language %>
<%= f.submit "Submit Question" %>
<%end%>
