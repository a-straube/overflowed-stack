<div id="questions-form">
  <%=form_for @question do |f| %>
  <%= f.label :Username %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :question %>
  <%= f.text_field :question %>
  <br>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <br>
  <%= f.label :language %>
  <%= f.text_field :language %>
  <br>
  <hr>
  <%= f.submit "Submit Question" %>
  <%end%>
</div>
